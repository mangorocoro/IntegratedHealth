<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dan.integratedhealth">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/smartphonefinger"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".HomeScreen"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FragmentViewer"
            android:label="@string/title_activity_fragment_viewer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ScenarioPicker"
            android:label="@string/title_activity_scenario_picker"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>